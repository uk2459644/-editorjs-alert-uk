{
  "name": "my-yarn-pack",
  "version": "1.0.0",
  "description": "to explore editorjs with yarn",
  "main": "index.js",
  "scripts": {
    "build": "webpack --mode production",
    "dev": "webpack --mode development --watch",
    "precommit": "yarn build && git add dist && lint-staged"
